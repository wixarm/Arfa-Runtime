{
  "name": "arfa-runtime",
  "version": "0.1.1",
  "author": "Arman Tarhani",
  "description": "Rendering engine for Arfa framework (jsx, renderer, router)",
  "type": "module",
  "main": "dist/runtime/index.js",
  "module": "dist/runtime/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/runtime/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsx": {
      "import": "./dist/runtime/jsx.js",
      "types": "./dist/runtime/jsx.d.ts"
    },
    "./renderer": {
      "import": "./dist/runtime/renderer.js",
      "types": "./dist/runtime/renderer.d.ts"
    },
    "./router": {
      "import": "./dist/runtime/router.js",
      "types": "./dist/runtime/router.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "jsx": [
        "dist/runtime/jsx.d.ts"
      ],
      "renderer": [
        "dist/runtime/renderer.d.ts"
      ],
      "router": [
        "dist/runtime/router.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "arfa",
    "runtime",
    "jsx",
    "renderer",
    "router"
  ],
  "license": "MIT",
  "peerDependencies": {
    "arfa-reactives": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.0"
  }
}
